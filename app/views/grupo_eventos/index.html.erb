<p id="notice"><%= notice %></p>

<h1>Grupos de Eventos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Prioridade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grupo_eventos.each do |grupo_evento| %>
      <tr>
        <td><%= grupo_evento.nome %></td>
        <td><%= get_priority(grupo_evento.prioridade) %></td>
        <td><%= link_to 'Show', grupo_evento %></td>
        <td><%= link_to 'Edit', edit_grupo_evento_path(grupo_evento) %></td>
        <td><%= link_to 'Destroy', grupo_evento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grupo Evento', new_grupo_evento_path %>
